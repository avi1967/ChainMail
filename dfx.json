{
  "version": 1,
  "canisters": {
    "chainmail_backend": {
      "type": "rust",
      "package": "chainmail_backend",
      "candid": ".dfx/local/canisters/chainmail_backend/chainmail_backend.did",
      "wasm": ".dfx/local/canisters/chainmail_backend/chainmail_backend.wasm"
    },
    "chainmail_frontend": {
      "type": "assets",
      "source": ["src/chainmail_frontend/dist"]
    }
  },
  "networks": {
    "local": { "bind": "127.0.0.1:4943", "type": "ephemeral" },
    "ic": { "providers": ["https://ic0.app"] }
  },
  "defaults": { "build": { "packtool": "" } }
}
